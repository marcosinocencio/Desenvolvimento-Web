<h3>Consultas: </h3>
<hr>
<form action="/searchresult" method="GET">
    <input type="search" name="search" id="search" placeholder="Digite o email ou o CPF do paciente">
    <button>Pesquisar</button>
</form>
<% appos.forEach(appo => { %>
    <h1>Consulta: <%= appo.description %></h1>    
    <h4>Paciente: <%= appo.name  %></h4>
    <h4>Email: <%= appo.email  %></h4>
    <h4>CPF: <%= appo.cpf  %></h4>
    <hr>
<% }) %>